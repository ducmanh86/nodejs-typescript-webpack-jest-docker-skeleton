{
  "extends": [
    "tslint-config-standard",
    "tslint:latest"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "trailing-comma": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "no-submodule-imports": false,
    "no-var-requires": false,
    "semicolon": {
      "options": "never"
    },
    "quotemark": {
      "options": "single"
    }
  }
}
